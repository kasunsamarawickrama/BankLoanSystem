@model BankLoanSystem.Models.DashBoard
@{
    ViewBag.Title = "UserDashBoard";
}

<h2>UserDashBoard </h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
  
    <div class="form-horizontal">
        @Html.ActionLink("My Profile", "SearchUsers", "DashBoard", new { type = 4 }, null)
    </div>
   
    if (Model.levelId == 1)
    {
        <div class="form-horizontal">

            @Html.ActionLink("Manage Super Admins", "SearchUsers", "DashBoard", new { type = 1 }, null)
        </div>
    }
    if (Model.levelId == 1 || Model.levelId == 2)
    {
        <div class="form-horizontal">
            @Html.ActionLink("Manage Admins", "SearchUsers", "DashBoard", new { type = 2 }, null)
        </div>
    }
    if (Model.levelId == 1 || Model.levelId == 2 || Model.levelId == 3)
    {
        <div class="form-horizontal">
            @Html.ActionLink("Manage Users", "SearchUsers", "DashBoard", new { type = 3 }, null)
        </div>
    }
   
}